{
  "name": "vue-sigplot",
  "version": "0.1.1",
  "description": "Vue wrapper for SigPlot",
  "main": "dist/vue-sigplot.common.js",
  "license": "Apache-2.0",
  "homepage": "https://github.com/lgsinnovations/vue-sigplot",
  "author": {
    "name": "LGS Innovations",
    "email": "sigplot@lgsinnovations.com",
    "url": "https://www.lgsinnovations.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lgsinnovations/vue-sigplot.git"
  },
  "bugs": {
    "url": "https://github.com/lgsinnovations/vue-sigplot/issues"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name vue-sigplot ./src/components/index.js",
    "test": "vue-cli-service test",
    "lint": "vue-cli-service lint",
    "prettier": "npx prettier --write *.js *.json src/**/*.js src/**/*.vue"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "sigplot": "^2.0.0-rc15",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.0.5",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
